<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.controller.booking.ClientInfoDialogController">
    <children>
        <GridPane layoutX="34.0" layoutY="54.0" prefHeight="247.0" prefWidth="523.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label text="Имя"/>
                <Label text="..." fx:id="name" GridPane.columnIndex="1"/>
                <Label text="Фамилия" GridPane.rowIndex="1"/>
                <Label text="..." fx:id="surname" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                <Label text="Отчество" GridPane.rowIndex="2"/>
                <Label text="..." fx:id="patronymic" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                <Label text="Дата рождения" GridPane.rowIndex="3"/>
                <Label text="..." fx:id="birthday" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
                <Label text="Эл. почта" GridPane.rowIndex="4"/>
                <Label text="..." fx:id="email" GridPane.columnIndex="1" GridPane.rowIndex="4"/>
                <Label text="Номер телефона" GridPane.rowIndex="5"/>
                <Label text="..." fx:id="phone" GridPane.columnIndex="1" GridPane.rowIndex="5"/>
            </children>
        </GridPane>
        <HBox alignment="CENTER_RIGHT" layoutX="357.0" layoutY="341.0" prefHeight="45.0" prefWidth="200.0" spacing="10">
            <children>
                <Button mnemonicParsing="false" text="Закрыть" onAction="#handleCloseAction"/>
            </children>
        </HBox>
    </children>
</AnchorPane>
