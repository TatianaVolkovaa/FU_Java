<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.controller.booking.TourInfoDialogController">
    <children>
        <GridPane layoutX="34.0" layoutY="54.0" prefHeight="247.0" prefWidth="523.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label text="Название"/>
                <Label text="..." fx:id="name" GridPane.columnIndex="1"/>
                <Label text="Дата начала" GridPane.rowIndex="1"/>
                <Label text="..." fx:id="beginDate" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                <Label text="Дата окончания" GridPane.rowIndex="2"/>
                <Label text="..." fx:id="endDate" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                <Label text="Цена за человека" GridPane.rowIndex="3"/>
                <Label text="..." fx:id="price" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
                <Label text="Описание" GridPane.rowIndex="4"/>
                <Label text="..." fx:id="description" GridPane.columnIndex="1" GridPane.rowIndex="4"/>
            </children>
        </GridPane>
        <HBox alignment="CENTER_RIGHT" layoutX="357.0" layoutY="341.0" prefHeight="45.0" prefWidth="200.0" spacing="10">
            <children>
                <Button mnemonicParsing="false" onAction="#handleCloseAction" text="Закрыть"/>
            </children>
        </HBox>
    </children>
</AnchorPane>
