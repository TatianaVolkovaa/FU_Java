<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.controller.client.TourHistoryDialogController">
    <children>
        <TableView fx:id="tourTable" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                   AnchorPane.rightAnchor="0.0"
                   AnchorPane.topAnchor="0.0">
            <columns>
                <TableColumn fx:id="nameColumn" prefWidth="136.0" text="Название"/>
                <TableColumn fx:id="beginDateColumn" prefWidth="128.0" text="Дата начала"/>
                <TableColumn fx:id="endDateColumn" prefWidth="156.0" text="Дата окончания"/>
                <TableColumn fx:id="priceColumn" prefWidth="184.0" text="Стоимость"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
    </children>
</AnchorPane>
