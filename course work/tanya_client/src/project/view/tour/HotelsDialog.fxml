<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="project.controller.tour.HotelsDialogController"
            prefHeight="400.0" prefWidth="600.0">
    <TableView fx:id="hotelTable" layoutX="1.0" prefHeight="598.0" prefWidth="582.0" AnchorPane.bottomAnchor="0.0"
               AnchorPane.leftAnchor=".0" AnchorPane.rightAnchor=".0" AnchorPane.topAnchor="0.0">
        <columns>
            <TableColumn fx:id="nameColumn" prefWidth="176.0" text="Название"/>
            <TableColumn fx:id="ratingColumn" minWidth="0.0" prefWidth="181.0" text="Рейтинг"/>
            <TableColumn fx:id="priceColumn" prefWidth="224.0" text="Цена за ночь"/>
        </columns>
        <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
        </columnResizePolicy>
    </TableView>
</AnchorPane>
