<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="project.controller.tour.CitiesDialogController"
            prefHeight="400.0" prefWidth="600.0">
    <TableView fx:id="cityTable" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0"
               AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0" prefHeight="600.0" prefWidth="900.0">
        <columns>
            <TableColumn fx:id="countryColumn" prefWidth="450.0" text="Страна"/>
            <TableColumn fx:id="cityColumn" prefWidth="449.0" text="Город"/>
        </columns>
        <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
        </columnResizePolicy>
    </TableView>
</AnchorPane>
