<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="project.controller.employee.EmployeeEditDialogController">
    <children>
        <GridPane layoutX="26.0" layoutY="25.0" prefHeight="316.0" prefWidth="547.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label text="Имя"/>
                <Label text="Фамилия" GridPane.rowIndex="1"/>
                <Label text="Отчество" GridPane.rowIndex="2"/>
                <Label text="Дата рождения" GridPane.rowIndex="3"/>
                <Label text="Пол" GridPane.rowIndex="4"/>
                <Label text="Эл. почта" GridPane.rowIndex="5"/>
                <TextField fx:id="name" GridPane.columnIndex="1"/>
                <TextField fx:id="surname" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                <TextField fx:id="patronymic" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
                <DatePicker fx:id="birthdate" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
                <TextField fx:id="gender" GridPane.columnIndex="1" GridPane.rowIndex="4"/>
                <TextField fx:id="email" GridPane.columnIndex="1" GridPane.rowIndex="5"/>
                <TextField fx:id="phone" GridPane.columnIndex="1" GridPane.rowIndex="6"/>
                <Label text="Телефон" GridPane.rowIndex="6"/>
                <Label text="Должность" GridPane.rowIndex="7"/>
                <Label text="Пароль" GridPane.rowIndex="8"/>
                <TextField GridPane.columnIndex="1" fx:id="pass" GridPane.rowIndex="8"/>
                <ChoiceBox prefWidth="150.0" fx:id="position" GridPane.columnIndex="1" GridPane.rowIndex="7"/>
            </children>
        </GridPane>
        <HBox alignment="CENTER_RIGHT" layoutX="373.0" layoutY="338.0" prefHeight="48.0" prefWidth="200.0" spacing="10">
            <children>
                <Button mnemonicParsing="false" onAction="#handleOkAction" text="Ок"/>
                <Button mnemonicParsing="false" onAction="#handleCancelAction" text="Отмена"/>
            </children>
        </HBox>
    </children>
</AnchorPane>
